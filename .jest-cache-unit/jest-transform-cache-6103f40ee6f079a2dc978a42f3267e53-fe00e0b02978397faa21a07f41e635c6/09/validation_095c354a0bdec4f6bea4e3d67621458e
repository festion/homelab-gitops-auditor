/**
 * Validation Middleware
 * Provides request validation for search and bulk action endpoints
 */

/**
 * Validate search query parameters
 */
function validateSearchQuery(req, res, next) {
  /* istanbul ignore next */
  cov_sims1js4k().f[0]++;
  const errors =
  /* istanbul ignore next */
  (cov_sims1js4k().s[0]++, []);

  // Validate limit
  /* istanbul ignore next */
  cov_sims1js4k().s[1]++;
  if (
  /* istanbul ignore next */
  (cov_sims1js4k().b[1][0]++, req.query.limit) &&
  /* istanbul ignore next */
  (cov_sims1js4k().b[1][1]++, isNaN(parseInt(req.query.limit)))) {
    /* istanbul ignore next */
    cov_sims1js4k().b[0][0]++;
    cov_sims1js4k().s[2]++;
    errors.push('Limit must be a number');
  } else {
    /* istanbul ignore next */
    cov_sims1js4k().b[0][1]++;
    cov_sims1js4k().s[3]++;
    if (
    /* istanbul ignore next */
    (cov_sims1js4k().b[3][0]++, req.query.limit) &&
    /* istanbul ignore next */
    (cov_sims1js4k().b[3][1]++, parseInt(req.query.limit) > 1000)) {
      /* istanbul ignore next */
      cov_sims1js4k().b[2][0]++;
      cov_sims1js4k().s[4]++;
      errors.push('Limit cannot exceed 1000');
    } else
    /* istanbul ignore next */
    {
      cov_sims1js4k().b[2][1]++;
    }
  }

  // Validate offset
  /* istanbul ignore next */
  cov_sims1js4k().s[5]++;
  if (
  /* istanbul ignore next */
  (cov_sims1js4k().b[5][0]++, req.query.offset) &&
  /* istanbul ignore next */
  (cov_sims1js4k().b[5][1]++, isNaN(parseInt(req.query.offset)))) {
    /* istanbul ignore next */
    cov_sims1js4k().b[4][0]++;
    cov_sims1js4k().s[6]++;
    errors.push('Offset must be a number');
  } else {
    /* istanbul ignore next */
    cov_sims1js4k().b[4][1]++;
    cov_sims1js4k().s[7]++;
    if (
    /* istanbul ignore next */
    (cov_sims1js4k().b[7][0]++, req.query.offset) &&
    /* istanbul ignore next */
    (cov_sims1js4k().b[7][1]++, parseInt(req.query.offset) < 0)) {
      /* istanbul ignore next */
      cov_sims1js4k().b[6][0]++;
      cov_sims1js4k().s[8]++;
      errors.push('Offset cannot be negative');
    } else
    /* istanbul ignore next */
    {
      cov_sims1js4k().b[6][1]++;
    }
  }

  // Validate sort direction
  /* istanbul ignore next */
  cov_sims1js4k().s[9]++;
  if (
  /* istanbul ignore next */
  (cov_sims1js4k().b[9][0]++, req.query.sort_direction) &&
  /* istanbul ignore next */
  (cov_sims1js4k().b[9][1]++, !['asc', 'desc'].includes(req.query.sort_direction))) {
    /* istanbul ignore next */
    cov_sims1js4k().b[8][0]++;
    cov_sims1js4k().s[10]++;
    errors.push('Sort direction must be "asc" or "desc"');
  } else
  /* istanbul ignore next */
  {
    cov_sims1js4k().b[8][1]++;
  }

  // Validate boolean parameters
  const booleanParams =
  /* istanbul ignore next */
  (cov_sims1js4k().s[11]++, ['fuzzy', 'highlight', 'include_facets', 'uncommitted_changes', 'stale_tags']);
  /* istanbul ignore next */
  cov_sims1js4k().s[12]++;
  for (const param of booleanParams) {
    /* istanbul ignore next */
    cov_sims1js4k().s[13]++;
    if (
    /* istanbul ignore next */
    (cov_sims1js4k().b[11][0]++, req.query[param]) &&
    /* istanbul ignore next */
    (cov_sims1js4k().b[11][1]++, !['true', 'false'].includes(req.query[param]))) {
      /* istanbul ignore next */
      cov_sims1js4k().b[10][0]++;
      cov_sims1js4k().s[14]++;
      errors.push(`${param} must be "true" or "false"`);
    } else
    /* istanbul ignore next */
    {
      cov_sims1js4k().b[10][1]++;
    }
  }

  // Validate last activity days
  /* istanbul ignore next */
  cov_sims1js4k().s[15]++;
  if (req.query.last_activity_days) {
    /* istanbul ignore next */
    cov_sims1js4k().b[12][0]++;
    const days =
    /* istanbul ignore next */
    (cov_sims1js4k().s[16]++, parseInt(req.query.last_activity_days));
    /* istanbul ignore next */
    cov_sims1js4k().s[17]++;
    if (
    /* istanbul ignore next */
    (cov_sims1js4k().b[14][0]++, isNaN(days)) ||
    /* istanbul ignore next */
    (cov_sims1js4k().b[14][1]++, days < 1) ||
    /* istanbul ignore next */
    (cov_sims1js4k().b[14][2]++, days > 365)) {
      /* istanbul ignore next */
      cov_sims1js4k().b[13][0]++;
      cov_sims1js4k().s[18]++;
      errors.push('Last activity days must be between 1 and 365');
    } else
    /* istanbul ignore next */
    {
      cov_sims1js4k().b[13][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_sims1js4k().b[12][1]++;
  }

  // Validate status values
  cov_sims1js4k().s[19]++;
  if (req.query.status) {
    /* istanbul ignore next */
    cov_sims1js4k().b[15][0]++;
    const validStatuses =
    /* istanbul ignore next */
    (cov_sims1js4k().s[20]++, ['clean', 'dirty', 'missing', 'extra']);
    const statuses =
    /* istanbul ignore next */
    (cov_sims1js4k().s[21]++, req.query.status.split(',').map(s => {
      /* istanbul ignore next */
      cov_sims1js4k().f[1]++;
      cov_sims1js4k().s[22]++;
      return s.trim();
    }));
    const invalidStatuses =
    /* istanbul ignore next */
    (cov_sims1js4k().s[23]++, statuses.filter(s => {
      /* istanbul ignore next */
      cov_sims1js4k().f[2]++;
      cov_sims1js4k().s[24]++;
      return !validStatuses.includes(s);
    }));
    /* istanbul ignore next */
    cov_sims1js4k().s[25]++;
    if (invalidStatuses.length > 0) {
      /* istanbul ignore next */
      cov_sims1js4k().b[16][0]++;
      cov_sims1js4k().s[26]++;
      errors.push(`Invalid status values: ${invalidStatuses.join(', ')}`);
    } else
    /* istanbul ignore next */
    {
      cov_sims1js4k().b[16][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_sims1js4k().b[15][1]++;
  }

  // Validate compliance values
  cov_sims1js4k().s[27]++;
  if (req.query.compliance) {
    /* istanbul ignore next */
    cov_sims1js4k().b[17][0]++;
    const validCompliance =
    /* istanbul ignore next */
    (cov_sims1js4k().s[28]++, ['compliant', 'non-compliant']);
    const compliance =
    /* istanbul ignore next */
    (cov_sims1js4k().s[29]++, req.query.compliance.split(',').map(c => {
      /* istanbul ignore next */
      cov_sims1js4k().f[3]++;
      cov_sims1js4k().s[30]++;
      return c.trim();
    }));
    const invalidCompliance =
    /* istanbul ignore next */
    (cov_sims1js4k().s[31]++, compliance.filter(c => {
      /* istanbul ignore next */
      cov_sims1js4k().f[4]++;
      cov_sims1js4k().s[32]++;
      return !validCompliance.includes(c);
    }));
    /* istanbul ignore next */
    cov_sims1js4k().s[33]++;
    if (invalidCompliance.length > 0) {
      /* istanbul ignore next */
      cov_sims1js4k().b[18][0]++;
      cov_sims1js4k().s[34]++;
      errors.push(`Invalid compliance values: ${invalidCompliance.join(', ')}`);
    } else
    /* istanbul ignore next */
    {
      cov_sims1js4k().b[18][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_sims1js4k().b[17][1]++;
  }

  // Validate pipeline status values
  cov_sims1js4k().s[35]++;
  if (req.query.pipeline_status) {
    /* istanbul ignore next */
    cov_sims1js4k().b[19][0]++;
    const validPipelineStatuses =
    /* istanbul ignore next */
    (cov_sims1js4k().s[36]++, ['success', 'failure', 'pending', 'running']);
    const pipelineStatuses =
    /* istanbul ignore next */
    (cov_sims1js4k().s[37]++, req.query.pipeline_status.split(',').map(p => {
      /* istanbul ignore next */
      cov_sims1js4k().f[5]++;
      cov_sims1js4k().s[38]++;
      return p.trim();
    }));
    const invalidPipelineStatuses =
    /* istanbul ignore next */
    (cov_sims1js4k().s[39]++, pipelineStatuses.filter(p => {
      /* istanbul ignore next */
      cov_sims1js4k().f[6]++;
      cov_sims1js4k().s[40]++;
      return !validPipelineStatuses.includes(p);
    }));
    /* istanbul ignore next */
    cov_sims1js4k().s[41]++;
    if (invalidPipelineStatuses.length > 0) {
      /* istanbul ignore next */
      cov_sims1js4k().b[20][0]++;
      cov_sims1js4k().s[42]++;
      errors.push(`Invalid pipeline status values: ${invalidPipelineStatuses.join(', ')}`);
    } else
    /* istanbul ignore next */
    {
      cov_sims1js4k().b[20][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_sims1js4k().b[19][1]++;
  }
  cov_sims1js4k().s[43]++;
  if (errors.length > 0) {
    /* istanbul ignore next */
    cov_sims1js4k().b[21][0]++;
    cov_sims1js4k().s[44]++;
    return res.status(400).json({
      error: 'Validation failed',
      message: 'Invalid query parameters',
      errors
    });
  } else
  /* istanbul ignore next */
  {
    cov_sims1js4k().b[21][1]++;
  }
  cov_sims1js4k().s[45]++;
  next();
}

/**
 * Validate bulk action parameters
 */
function validateBulkActionParams(req, res, next) {
  /* istanbul ignore next */
  cov_sims1js4k().f[7]++;
  const errors =
  /* istanbul ignore next */
  (cov_sims1js4k().s[46]++, []);
  const {
    type,
    repositories,
    params
  } =
  /* istanbul ignore next */
  (cov_sims1js4k().s[47]++, req.body);

  // Validate required fields
  /* istanbul ignore next */
  cov_sims1js4k().s[48]++;
  if (!type) {
    /* istanbul ignore next */
    cov_sims1js4k().b[22][0]++;
    cov_sims1js4k().s[49]++;
    errors.push('Action type is required');
  } else
  /* istanbul ignore next */
  {
    cov_sims1js4k().b[22][1]++;
  }
  cov_sims1js4k().s[50]++;
  if (!repositories) {
    /* istanbul ignore next */
    cov_sims1js4k().b[23][0]++;
    cov_sims1js4k().s[51]++;
    errors.push('Repositories array is required');
  } else {
    /* istanbul ignore next */
    cov_sims1js4k().b[23][1]++;
    cov_sims1js4k().s[52]++;
    if (!Array.isArray(repositories)) {
      /* istanbul ignore next */
      cov_sims1js4k().b[24][0]++;
      cov_sims1js4k().s[53]++;
      errors.push('Repositories must be an array');
    } else {
      /* istanbul ignore next */
      cov_sims1js4k().b[24][1]++;
      cov_sims1js4k().s[54]++;
      if (repositories.length === 0) {
        /* istanbul ignore next */
        cov_sims1js4k().b[25][0]++;
        cov_sims1js4k().s[55]++;
        errors.push('At least one repository must be specified');
      } else {
        /* istanbul ignore next */
        cov_sims1js4k().b[25][1]++;
        cov_sims1js4k().s[56]++;
        if (repositories.length > 100) {
          /* istanbul ignore next */
          cov_sims1js4k().b[26][0]++;
          cov_sims1js4k().s[57]++;
          errors.push('Cannot process more than 100 repositories at once');
        } else
        /* istanbul ignore next */
        {
          cov_sims1js4k().b[26][1]++;
        }
      }
    }
  }

  // Validate repository names
  /* istanbul ignore next */
  cov_sims1js4k().s[58]++;
  if (Array.isArray(repositories)) {
    /* istanbul ignore next */
    cov_sims1js4k().b[27][0]++;
    cov_sims1js4k().s[59]++;
    repositories.forEach((repo, index) => {
      /* istanbul ignore next */
      cov_sims1js4k().f[8]++;
      cov_sims1js4k().s[60]++;
      if (
      /* istanbul ignore next */
      (cov_sims1js4k().b[29][0]++, typeof repo !== 'string') ||
      /* istanbul ignore next */
      (cov_sims1js4k().b[29][1]++, repo.trim().length === 0)) {
        /* istanbul ignore next */
        cov_sims1js4k().b[28][0]++;
        cov_sims1js4k().s[61]++;
        errors.push(`Repository at index ${index} must be a non-empty string`);
      } else
      /* istanbul ignore next */
      {
        cov_sims1js4k().b[28][1]++;
      }
    });
  } else
  /* istanbul ignore next */
  {
    cov_sims1js4k().b[27][1]++;
  }

  // Validate action type
  const validActionTypes =
  /* istanbul ignore next */
  (cov_sims1js4k().s[62]++, ['apply_templates', 'run_audit', 'export_data', 'tag_repositories']);
  /* istanbul ignore next */
  cov_sims1js4k().s[63]++;
  if (
  /* istanbul ignore next */
  (cov_sims1js4k().b[31][0]++, type) &&
  /* istanbul ignore next */
  (cov_sims1js4k().b[31][1]++, !validActionTypes.includes(type))) {
    /* istanbul ignore next */
    cov_sims1js4k().b[30][0]++;
    cov_sims1js4k().s[64]++;
    errors.push(`Invalid action type. Must be one of: ${validActionTypes.join(', ')}`);
  } else
  /* istanbul ignore next */
  {
    cov_sims1js4k().b[30][1]++;
  }

  // Type-specific validation
  cov_sims1js4k().s[65]++;
  if (type === 'apply_templates') {
    /* istanbul ignore next */
    cov_sims1js4k().b[32][0]++;
    cov_sims1js4k().s[66]++;
    if (
    /* istanbul ignore next */
    (cov_sims1js4k().b[34][0]++, !params) ||
    /* istanbul ignore next */
    (cov_sims1js4k().b[34][1]++, !params.templates) ||
    /* istanbul ignore next */
    (cov_sims1js4k().b[34][2]++, !Array.isArray(params.templates))) {
      /* istanbul ignore next */
      cov_sims1js4k().b[33][0]++;
      cov_sims1js4k().s[67]++;
      errors.push('Templates array is required for apply_templates action');
    } else {
      /* istanbul ignore next */
      cov_sims1js4k().b[33][1]++;
      cov_sims1js4k().s[68]++;
      if (params.templates.length === 0) {
        /* istanbul ignore next */
        cov_sims1js4k().b[35][0]++;
        cov_sims1js4k().s[69]++;
        errors.push('At least one template must be specified');
      } else
      /* istanbul ignore next */
      {
        cov_sims1js4k().b[35][1]++;
      }
    }
  } else
  /* istanbul ignore next */
  {
    cov_sims1js4k().b[32][1]++;
  }
  cov_sims1js4k().s[70]++;
  if (type === 'run_audit') {
    /* istanbul ignore next */
    cov_sims1js4k().b[36][0]++;
    cov_sims1js4k().s[71]++;
    if (
    /* istanbul ignore next */
    (cov_sims1js4k().b[38][0]++, params) &&
    /* istanbul ignore next */
    (cov_sims1js4k().b[38][1]++, params.type)) {
      /* istanbul ignore next */
      cov_sims1js4k().b[37][0]++;
      const validAuditTypes =
      /* istanbul ignore next */
      (cov_sims1js4k().s[72]++, ['full', 'security', 'compliance']);
      /* istanbul ignore next */
      cov_sims1js4k().s[73]++;
      if (!validAuditTypes.includes(params.type)) {
        /* istanbul ignore next */
        cov_sims1js4k().b[39][0]++;
        cov_sims1js4k().s[74]++;
        errors.push(`Invalid audit type. Must be one of: ${validAuditTypes.join(', ')}`);
      } else
      /* istanbul ignore next */
      {
        cov_sims1js4k().b[39][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_sims1js4k().b[37][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_sims1js4k().b[36][1]++;
  }
  cov_sims1js4k().s[75]++;
  if (type === 'export_data') {
    /* istanbul ignore next */
    cov_sims1js4k().b[40][0]++;
    cov_sims1js4k().s[76]++;
    if (
    /* istanbul ignore next */
    (cov_sims1js4k().b[42][0]++, params) &&
    /* istanbul ignore next */
    (cov_sims1js4k().b[42][1]++, params.format)) {
      /* istanbul ignore next */
      cov_sims1js4k().b[41][0]++;
      const validFormats =
      /* istanbul ignore next */
      (cov_sims1js4k().s[77]++, ['json', 'csv', 'excel']);
      /* istanbul ignore next */
      cov_sims1js4k().s[78]++;
      if (!validFormats.includes(params.format)) {
        /* istanbul ignore next */
        cov_sims1js4k().b[43][0]++;
        cov_sims1js4k().s[79]++;
        errors.push(`Invalid export format. Must be one of: ${validFormats.join(', ')}`);
      } else
      /* istanbul ignore next */
      {
        cov_sims1js4k().b[43][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_sims1js4k().b[41][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_sims1js4k().b[40][1]++;
  }
  cov_sims1js4k().s[80]++;
  if (type === 'tag_repositories') {
    /* istanbul ignore next */
    cov_sims1js4k().b[44][0]++;
    cov_sims1js4k().s[81]++;
    if (
    /* istanbul ignore next */
    (cov_sims1js4k().b[46][0]++, !params) ||
    /* istanbul ignore next */
    (cov_sims1js4k().b[46][1]++, !params.operation)) {
      /* istanbul ignore next */
      cov_sims1js4k().b[45][0]++;
      cov_sims1js4k().s[82]++;
      errors.push('Operation is required for tag_repositories action');
    } else {
      /* istanbul ignore next */
      cov_sims1js4k().b[45][1]++;
      cov_sims1js4k().s[83]++;
      if (!['add', 'remove'].includes(params.operation)) {
        /* istanbul ignore next */
        cov_sims1js4k().b[47][0]++;
        cov_sims1js4k().s[84]++;
        errors.push('Operation must be either "add" or "remove"');
      } else
      /* istanbul ignore next */
      {
        cov_sims1js4k().b[47][1]++;
      }
    }
    /* istanbul ignore next */
    cov_sims1js4k().s[85]++;
    if (
    /* istanbul ignore next */
    (cov_sims1js4k().b[49][0]++, !params) ||
    /* istanbul ignore next */
    (cov_sims1js4k().b[49][1]++, !params.tags) ||
    /* istanbul ignore next */
    (cov_sims1js4k().b[49][2]++, typeof params.tags !== 'string') ||
    /* istanbul ignore next */
    (cov_sims1js4k().b[49][3]++, params.tags.trim().length === 0)) {
      /* istanbul ignore next */
      cov_sims1js4k().b[48][0]++;
      cov_sims1js4k().s[86]++;
      errors.push('Tags string is required for tag_repositories action');
    } else
    /* istanbul ignore next */
    {
      cov_sims1js4k().b[48][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_sims1js4k().b[44][1]++;
  }
  cov_sims1js4k().s[87]++;
  if (errors.length > 0) {
    /* istanbul ignore next */
    cov_sims1js4k().b[50][0]++;
    cov_sims1js4k().s[88]++;
    return res.status(400).json({
      error: 'Validation failed',
      message: 'Invalid bulk action parameters',
      errors
    });
  } else
  /* istanbul ignore next */
  {
    cov_sims1js4k().b[50][1]++;
  }
  cov_sims1js4k().s[89]++;
  next();
}

/**
 * Validate saved view parameters
 */
function validateSavedViewParams(req, res, next) {
  /* istanbul ignore next */
  cov_sims1js4k().f[9]++;
  const errors =
  /* istanbul ignore next */
  (cov_sims1js4k().s[90]++, []);
  const {
    name,
    criteria,
    isDefault
  } =
  /* istanbul ignore next */
  (cov_sims1js4k().s[91]++, req.body);

  // Validate name
  /* istanbul ignore next */
  cov_sims1js4k().s[92]++;
  if (!name) {
    /* istanbul ignore next */
    cov_sims1js4k().b[51][0]++;
    cov_sims1js4k().s[93]++;
    errors.push('View name is required');
  } else {
    /* istanbul ignore next */
    cov_sims1js4k().b[51][1]++;
    cov_sims1js4k().s[94]++;
    if (typeof name !== 'string') {
      /* istanbul ignore next */
      cov_sims1js4k().b[52][0]++;
      cov_sims1js4k().s[95]++;
      errors.push('View name must be a string');
    } else {
      /* istanbul ignore next */
      cov_sims1js4k().b[52][1]++;
      cov_sims1js4k().s[96]++;
      if (name.trim().length === 0) {
        /* istanbul ignore next */
        cov_sims1js4k().b[53][0]++;
        cov_sims1js4k().s[97]++;
        errors.push('View name cannot be empty');
      } else {
        /* istanbul ignore next */
        cov_sims1js4k().b[53][1]++;
        cov_sims1js4k().s[98]++;
        if (name.length > 100) {
          /* istanbul ignore next */
          cov_sims1js4k().b[54][0]++;
          cov_sims1js4k().s[99]++;
          errors.push('View name cannot exceed 100 characters');
        } else
        /* istanbul ignore next */
        {
          cov_sims1js4k().b[54][1]++;
        }
      }
    }
  }

  // Validate criteria
  /* istanbul ignore next */
  cov_sims1js4k().s[100]++;
  if (!criteria) {
    /* istanbul ignore next */
    cov_sims1js4k().b[55][0]++;
    cov_sims1js4k().s[101]++;
    errors.push('Search criteria is required');
  } else {
    /* istanbul ignore next */
    cov_sims1js4k().b[55][1]++;
    cov_sims1js4k().s[102]++;
    if (typeof criteria !== 'object') {
      /* istanbul ignore next */
      cov_sims1js4k().b[56][0]++;
      cov_sims1js4k().s[103]++;
      errors.push('Search criteria must be an object');
    } else {
      /* istanbul ignore next */
      cov_sims1js4k().b[56][1]++;
      cov_sims1js4k().s[104]++;
      // Validate criteria structure
      if (
      /* istanbul ignore next */
      (cov_sims1js4k().b[58][0]++, criteria.query !== undefined) &&
      /* istanbul ignore next */
      (cov_sims1js4k().b[58][1]++, typeof criteria.query !== 'string')) {
        /* istanbul ignore next */
        cov_sims1js4k().b[57][0]++;
        cov_sims1js4k().s[105]++;
        errors.push('Search query must be a string');
      } else
      /* istanbul ignore next */
      {
        cov_sims1js4k().b[57][1]++;
      }
      cov_sims1js4k().s[106]++;
      if (
      /* istanbul ignore next */
      (cov_sims1js4k().b[60][0]++, criteria.filters !== undefined) &&
      /* istanbul ignore next */
      (cov_sims1js4k().b[60][1]++, typeof criteria.filters !== 'object')) {
        /* istanbul ignore next */
        cov_sims1js4k().b[59][0]++;
        cov_sims1js4k().s[107]++;
        errors.push('Filters must be an object');
      } else
      /* istanbul ignore next */
      {
        cov_sims1js4k().b[59][1]++;
      }
      cov_sims1js4k().s[108]++;
      if (criteria.sort !== undefined) {
        /* istanbul ignore next */
        cov_sims1js4k().b[61][0]++;
        cov_sims1js4k().s[109]++;
        if (typeof criteria.sort !== 'object') {
          /* istanbul ignore next */
          cov_sims1js4k().b[62][0]++;
          cov_sims1js4k().s[110]++;
          errors.push('Sort must be an object');
        } else {
          /* istanbul ignore next */
          cov_sims1js4k().b[62][1]++;
          cov_sims1js4k().s[111]++;
          if (
          /* istanbul ignore next */
          (cov_sims1js4k().b[64][0]++, criteria.sort.field) &&
          /* istanbul ignore next */
          (cov_sims1js4k().b[64][1]++, typeof criteria.sort.field !== 'string')) {
            /* istanbul ignore next */
            cov_sims1js4k().b[63][0]++;
            cov_sims1js4k().s[112]++;
            errors.push('Sort field must be a string');
          } else
          /* istanbul ignore next */
          {
            cov_sims1js4k().b[63][1]++;
          }
          cov_sims1js4k().s[113]++;
          if (
          /* istanbul ignore next */
          (cov_sims1js4k().b[66][0]++, criteria.sort.direction) &&
          /* istanbul ignore next */
          (cov_sims1js4k().b[66][1]++, !['asc', 'desc'].includes(criteria.sort.direction))) {
            /* istanbul ignore next */
            cov_sims1js4k().b[65][0]++;
            cov_sims1js4k().s[114]++;
            errors.push('Sort direction must be "asc" or "desc"');
          } else
          /* istanbul ignore next */
          {
            cov_sims1js4k().b[65][1]++;
          }
        }
      } else
      /* istanbul ignore next */
      {
        cov_sims1js4k().b[61][1]++;
      }
    }
  }

  // Validate isDefault
  /* istanbul ignore next */
  cov_sims1js4k().s[115]++;
  if (
  /* istanbul ignore next */
  (cov_sims1js4k().b[68][0]++, isDefault !== undefined) &&
  /* istanbul ignore next */
  (cov_sims1js4k().b[68][1]++, typeof isDefault !== 'boolean')) {
    /* istanbul ignore next */
    cov_sims1js4k().b[67][0]++;
    cov_sims1js4k().s[116]++;
    errors.push('isDefault must be a boolean');
  } else
  /* istanbul ignore next */
  {
    cov_sims1js4k().b[67][1]++;
  }
  cov_sims1js4k().s[117]++;
  if (errors.length > 0) {
    /* istanbul ignore next */
    cov_sims1js4k().b[69][0]++;
    cov_sims1js4k().s[118]++;
    return res.status(400).json({
      error: 'Validation failed',
      message: 'Invalid saved view parameters',
      errors
    });
  } else
  /* istanbul ignore next */
  {
    cov_sims1js4k().b[69][1]++;
  }
  cov_sims1js4k().s[119]++;
  next();
}

/**
 * Validate pagination parameters
 */
function validatePagination(req, res, next) {
  /* istanbul ignore next */
  cov_sims1js4k().f[10]++;
  const errors =
  /* istanbul ignore next */
  (cov_sims1js4k().s[120]++, []);
  /* istanbul ignore next */
  cov_sims1js4k().s[121]++;
  if (req.query.page) {
    /* istanbul ignore next */
    cov_sims1js4k().b[70][0]++;
    const page =
    /* istanbul ignore next */
    (cov_sims1js4k().s[122]++, parseInt(req.query.page));
    /* istanbul ignore next */
    cov_sims1js4k().s[123]++;
    if (
    /* istanbul ignore next */
    (cov_sims1js4k().b[72][0]++, isNaN(page)) ||
    /* istanbul ignore next */
    (cov_sims1js4k().b[72][1]++, page < 1)) {
      /* istanbul ignore next */
      cov_sims1js4k().b[71][0]++;
      cov_sims1js4k().s[124]++;
      errors.push('Page must be a positive integer');
    } else {
      /* istanbul ignore next */
      cov_sims1js4k().b[71][1]++;
      cov_sims1js4k().s[125]++;
      if (page > 10000) {
        /* istanbul ignore next */
        cov_sims1js4k().b[73][0]++;
        cov_sims1js4k().s[126]++;
        errors.push('Page cannot exceed 10000');
      } else
      /* istanbul ignore next */
      {
        cov_sims1js4k().b[73][1]++;
      }
    }
  } else
  /* istanbul ignore next */
  {
    cov_sims1js4k().b[70][1]++;
  }
  cov_sims1js4k().s[127]++;
  if (req.query.limit) {
    /* istanbul ignore next */
    cov_sims1js4k().b[74][0]++;
    const limit =
    /* istanbul ignore next */
    (cov_sims1js4k().s[128]++, parseInt(req.query.limit));
    /* istanbul ignore next */
    cov_sims1js4k().s[129]++;
    if (
    /* istanbul ignore next */
    (cov_sims1js4k().b[76][0]++, isNaN(limit)) ||
    /* istanbul ignore next */
    (cov_sims1js4k().b[76][1]++, limit < 1)) {
      /* istanbul ignore next */
      cov_sims1js4k().b[75][0]++;
      cov_sims1js4k().s[130]++;
      errors.push('Limit must be a positive integer');
    } else {
      /* istanbul ignore next */
      cov_sims1js4k().b[75][1]++;
      cov_sims1js4k().s[131]++;
      if (limit > 1000) {
        /* istanbul ignore next */
        cov_sims1js4k().b[77][0]++;
        cov_sims1js4k().s[132]++;
        errors.push('Limit cannot exceed 1000');
      } else
      /* istanbul ignore next */
      {
        cov_sims1js4k().b[77][1]++;
      }
    }
  } else
  /* istanbul ignore next */
  {
    cov_sims1js4k().b[74][1]++;
  }
  cov_sims1js4k().s[133]++;
  if (errors.length > 0) {
    /* istanbul ignore next */
    cov_sims1js4k().b[78][0]++;
    cov_sims1js4k().s[134]++;
    return res.status(400).json({
      error: 'Validation failed',
      message: 'Invalid pagination parameters',
      errors
    });
  } else
  /* istanbul ignore next */
  {
    cov_sims1js4k().b[78][1]++;
  }
  cov_sims1js4k().s[135]++;
  next();
}

/**
 * Sanitize input to prevent XSS and injection attacks
 */
function sanitizeInput(req, res, next) {
  /* istanbul ignore next */
  cov_sims1js4k().f[11]++;
  cov_sims1js4k().s[136]++;
  const sanitizeString = str => {
    /* istanbul ignore next */
    cov_sims1js4k().f[12]++;
    cov_sims1js4k().s[137]++;
    if (typeof str !== 'string') {
      /* istanbul ignore next */
      cov_sims1js4k().b[79][0]++;
      cov_sims1js4k().s[138]++;
      return str;
    } else
    /* istanbul ignore next */
    {
      cov_sims1js4k().b[79][1]++;
    }

    // Remove script tags and other dangerous HTML
    cov_sims1js4k().s[139]++;
    return str.replace(/<script\b[^<]*(?:(?!<\/script>)<[^<]*)*<\/script>/gi, '').replace(/<iframe\b[^<]*(?:(?!<\/iframe>)<[^<]*)*<\/iframe>/gi, '').replace(/javascript:/gi, '').replace(/on\w+=/gi, '').trim();
  };
  /* istanbul ignore next */
  cov_sims1js4k().s[140]++;
  const sanitizeObject = obj => {
    /* istanbul ignore next */
    cov_sims1js4k().f[13]++;
    cov_sims1js4k().s[141]++;
    if (
    /* istanbul ignore next */
    (cov_sims1js4k().b[81][0]++, typeof obj !== 'object') ||
    /* istanbul ignore next */
    (cov_sims1js4k().b[81][1]++, obj === null)) {
      /* istanbul ignore next */
      cov_sims1js4k().b[80][0]++;
      cov_sims1js4k().s[142]++;
      return obj;
    } else
    /* istanbul ignore next */
    {
      cov_sims1js4k().b[80][1]++;
    }
    cov_sims1js4k().s[143]++;
    if (Array.isArray(obj)) {
      /* istanbul ignore next */
      cov_sims1js4k().b[82][0]++;
      cov_sims1js4k().s[144]++;
      return obj.map(sanitizeObject);
    } else
    /* istanbul ignore next */
    {
      cov_sims1js4k().b[82][1]++;
    }
    const sanitized =
    /* istanbul ignore next */
    (cov_sims1js4k().s[145]++, {});
    /* istanbul ignore next */
    cov_sims1js4k().s[146]++;
    for (const [key, value] of Object.entries(obj)) {
      /* istanbul ignore next */
      cov_sims1js4k().s[147]++;
      if (typeof value === 'string') {
        /* istanbul ignore next */
        cov_sims1js4k().b[83][0]++;
        cov_sims1js4k().s[148]++;
        sanitized[key] = sanitizeString(value);
      } else {
        /* istanbul ignore next */
        cov_sims1js4k().b[83][1]++;
        cov_sims1js4k().s[149]++;
        if (typeof value === 'object') {
          /* istanbul ignore next */
          cov_sims1js4k().b[84][0]++;
          cov_sims1js4k().s[150]++;
          sanitized[key] = sanitizeObject(value);
        } else {
          /* istanbul ignore next */
          cov_sims1js4k().b[84][1]++;
          cov_sims1js4k().s[151]++;
          sanitized[key] = value;
        }
      }
    }
    /* istanbul ignore next */
    cov_sims1js4k().s[152]++;
    return sanitized;
  };

  // Sanitize query parameters
  /* istanbul ignore next */
  cov_sims1js4k().s[153]++;
  if (req.query) {
    /* istanbul ignore next */
    cov_sims1js4k().b[85][0]++;
    cov_sims1js4k().s[154]++;
    req.query = sanitizeObject(req.query);
  } else
  /* istanbul ignore next */
  {
    cov_sims1js4k().b[85][1]++;
  }

  // Sanitize body parameters
  cov_sims1js4k().s[155]++;
  if (req.body) {
    /* istanbul ignore next */
    cov_sims1js4k().b[86][0]++;
    cov_sims1js4k().s[156]++;
    req.body = sanitizeObject(req.body);
  } else
  /* istanbul ignore next */
  {
    cov_sims1js4k().b[86][1]++;
  }
  cov_sims1js4k().s[157]++;
  next();
}
/* istanbul ignore next */
cov_sims1js4k().s[158]++;
module.exports = {
  validateSearchQuery,
  validateBulkActionParams,
  validateSavedViewParams,
  validatePagination,
  sanitizeInput
};
